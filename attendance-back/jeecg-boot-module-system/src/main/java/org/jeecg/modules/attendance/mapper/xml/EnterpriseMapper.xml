<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.attendance.mapper.EnterpriseMapper">

	<select id="queryEnterpriseList" parameterType="map" resultType="map">
		select id,enterprise_name,enterprise_owner,enterprise_contact,enterprise_type,enterprise_app,enterprise_address,create_by,create_time,modify_by,modify_time
		from attendance_enterprise
		 limit
        ${start},${pageSize}
	</select>

	<select id="queryEnterpriseListTotal" parameterType="map" resultType="java.lang.Long">
		select count(1)
		from attendance_enterprise
	</select>

</mapper>